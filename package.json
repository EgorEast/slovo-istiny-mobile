{
  "name": "slovo-istiny-mobile",
  "version": "1.0.5",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc -p tsconfig.json",
    "prettier:write": "prettier --write **/*.{js,jsx,ts,tsx,json} && prettier --write *.{js,jsx,ts,tsx,json}",
    "emulator-run-builds": "eas build:run -p android",
    "update-app": "eas update --branch main --message \"Updating the app\"",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build-preview:android": "eas build -p android --profile preview",
    "build-preview:ios": "eas build -p ios --profile preview",
    "build-preview:all": "eas build -p all --profile preview",
    "build:list": "eas build:list",
    "version-patch": "yarn version --patch",
    "version-minor": "yarn version --minor",
    "version-major": "yarn version --major"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "expo": "^50.0.4",
    "expo-av": "~13.10.4",
    "expo-checkbox": "~2.7.0",
    "expo-constants": "~15.4.5",
    "expo-device": "~5.9.3",
    "expo-file-system": "~16.0.5",
    "expo-intent-launcher": "~10.11.0",
    "expo-notifications": "~0.27.6",
    "expo-status-bar": "~1.11.1",
    "expo-task-manager": "~11.7.2",
    "expo-updates": "~0.24.9",
    "jest": "^29.7.0",
    "jest-expo": "^50.0.1",
    "ky": "1.2.0",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-gesture-handler": "~2.14.1",
    "react-native-pager-view": "6.2.3",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-tab-view": "^3.5.2",
    "xml-js": "^1.6.11",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.23.9",
    "@expo/webpack-config": "^19.0.1",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.48",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.2.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.3",
    "eslint-plugin-perfectionist": "^2.5.0",
    "eslint-plugin-sonarjs": "^0.23.0",
    "prettier": "^3.2.4",
    "react-dom": "18.2.0",
    "react-native-web": "~0.19.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
