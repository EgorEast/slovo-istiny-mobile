{
  "name": "slovo-istiny-mobile",
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "ci": "rm -rf node_modules && yarn install --frozen-lockfile",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier:write": "npx prettier --write **/*.{js,jsx,ts,tsx,json} && npx prettier --write *.{js,jsx,ts,tsx,json}",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build-preview:android": "eas build -p android --profile preview",
    "build:list": "eas build:list"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.8",
    "@react-navigation/native": "^6.1.7",
    "@react-navigation/native-stack": "^6.9.13",
    "expo": "~48.0.19",
    "expo-checkbox": "^2.3.1",
    "expo-file-system": "~15.2.2",
    "expo-intent-launcher": "~10.5.2",
    "expo-status-bar": "~1.4.4",
    "jest": "^29.5.0",
    "jest-expo": "^48.0.2",
    "ky": "^0.33.3",
    "react": "18.2.0",
    "react-native": "0.71.8",
    "react-native-pager-view": "6.1.2",
    "react-native-safe-area-context": "4.5.0",
    "react-native-screens": "~3.20.0",
    "react-native-tab-view": "^3.5.2",
    "zustand": "^4.3.8"
  },
  "devDependencies": {
    "@babel/core": "^7.22.6",
    "@expo/webpack-config": "^18.1.0",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@testing-library/jest-native": "^5.4.2",
    "@testing-library/react-native": "^12.1.2",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/jest": "^29.5.2",
    "@types/react": "~18.0.27",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.44.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-boundaries": "^3.1.1",
    "eslint-plugin-import": "^2.27.5",
    "react-dom": "18.2.0",
    "react-native-web": "~0.18.10",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "private": true
}
