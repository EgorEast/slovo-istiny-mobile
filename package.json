{
  "name": "slovo-istiny-mobile",
  "version": "1.0.5",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "test": "jest --watch --coverage=false --changedSince=origin/main",
    "testDebug": "jest -o --watch --coverage=false",
    "testFinal": "jest",
    "updateSnapshots": "jest -u --coverage=false",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check-types": "tsc -p tsconfig.json",
    "prettier:write": "prettier --write **/*.{js,jsx,ts,tsx,json} && prettier --write *.{js,jsx,ts,tsx,json}",
    "emulator-run-builds": "eas build:run -p android",
    "update-app": "eas update --branch main --message \"Updating the app\"",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "build:all": "eas build --platform all",
    "build-preview:android": "eas build -p android --profile preview",
    "build-preview:ios": "eas build -p ios --profile preview",
    "build-preview:all": "eas build -p all --profile preview",
    "build:list": "eas build:list",
    "version-patch": "yarn version --patch",
    "version-minor": "yarn version --minor",
    "version-major": "yarn version --major"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.18.2",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/native-stack": "^6.9.17",
    "expo": "^49.0.21",
    "expo-av": "~13.4.1",
    "expo-checkbox": "~2.4.0",
    "expo-constants": "~14.4.2",
    "expo-device": "~5.4.0",
    "expo-file-system": "~15.4.5",
    "expo-intent-launcher": "~10.7.0",
    "expo-notifications": "~0.20.1",
    "expo-status-bar": "~1.6.0",
    "expo-task-manager": "~11.3.0",
    "expo-updates": "~0.18.19",
    "jest": "^29.7.0",
    "jest-expo": "^49.0.0",
    "ky": "1.1.3",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-gesture-handler": "~2.12.0",
    "react-native-pager-view": "6.2.0",
    "react-native-safe-area-context": "4.6.3",
    "react-native-screens": "~3.22.0",
    "react-native-tab-view": "^3.5.2",
    "xml-js": "^1.6.11",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@expo/webpack-config": "^19.0.0",
    "@feature-sliced/eslint-config": "^0.1.0-beta.6",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/jest": "^29.5.11",
    "@types/react": "~18.2.46",
    "@types/react-test-renderer": "^18.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.56.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-boundaries": "^4.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.6.1",
    "eslint-plugin-perfectionist": "^2.5.0",
    "prettier": "^3.1.1",
    "react-dom": "18.2.0",
    "react-native-web": "~0.19.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
